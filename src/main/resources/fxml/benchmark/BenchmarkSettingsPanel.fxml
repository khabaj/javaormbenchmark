<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXSlider?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="200.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.khabaj.ormbenchmark.launcher.controllers.benchmark.settings.BenchmarkSettingsCtrl">
   <children>
      <Label text="Benchmark Setting:">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <HBox>
         <children>
            <VBox spacing="10.0">
               <children>
                  <Label text="Benchmarks to run:" />
                  <JFXTreeView fx:id="banchmarksTreeView" />
               </children>
            </VBox>
            <VBox spacing="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="FORKS:" textFill="#727272" />
                  <Spinner />
                  <Label text="Warmup Iterations:" textFill="#727272" />
                  <JFXComboBox prefHeight="25.0" prefWidth="231.0" />
                  <Label text="Measurement Iterations:" textFill="#727272" />
                  <JFXTextField />
                  <Label prefHeight="21.0" text="Time Unit:" textFill="#727272" />
                  <JFXSlider />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
